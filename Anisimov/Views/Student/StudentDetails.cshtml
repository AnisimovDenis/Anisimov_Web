@model Anisimov.ViewModels.StudentViewModel

@Html.ActionLink("Назад к списку", "Students", "Student")

<table class="table table-bordered">
    <tr>
        <td>Id</td>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <td>Фамилия</td>
        <td>@Model.SurName</td>
    </tr>
    <tr>
        <td>Имя</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Возраст</td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td>Должность</td>
        <td>@Model.Class</td>
    </tr>
</table>